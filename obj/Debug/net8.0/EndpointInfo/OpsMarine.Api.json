  "components": {
    "schemas": {
      "FuelInspection": {
        "required": [
          "vessel"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "vessel": {
            "maxLength": 128,
            "minLength": 1,
            "type": "string"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "hullFouling": {
            "maximum": 5,
            "minimum": 0,
            "type": "integer",
            "format": "int32"
          },
          "propeller": {
            "maximum": 5,
            "minimum": 0,
            "type": "integer",
            "format": "int32"
          },
          "engine": {
            "maximum": 5,
            "minimum": 0,
            "type": "integer",
            "format": "int32"
          },
          "trimOk": {
            "type": "boolean"
          },
          "excessBallast": {
            "type": "boolean"
          },
          "notes": {
            "maxLength": 512,
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "FuelPatch": {
        "type": "object",
        "properties": {
          "liters": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "percent": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}