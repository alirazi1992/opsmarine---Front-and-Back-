    "/fuel/{id}": {
      "patch": {
        "tags": [
          "Fuel"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/FuelPatch"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/FuelPatch"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/FuelPatch"
              }
            }
          }
        },
